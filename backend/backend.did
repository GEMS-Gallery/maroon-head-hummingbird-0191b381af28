type Result_1 = 
 variant {
   err: text;
   ok;
 };
type Result = 
 variant {
   err: text;
   ok: nat;
 };
type Folder = 
 record {
   id: nat;
   name: text;
 };
type File = 
 record {
   category: text;
   fileType: text;
   id: nat;
   name: text;
   size: opt nat;
 };
service : {
  createFolder: (text) -> (Result);
  deleteFile: (nat) -> (Result_1);
  deleteFolder: (nat) -> (Result_1);
  getFiles: () -> (vec File) query;
  getFolders: () -> (vec Folder) query;
  uploadFile: (text, text, opt nat, text) -> (Result);
}
